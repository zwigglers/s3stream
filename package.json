{
  "name": "s3-streamer",
  "version": "1.0.2",
  "description": "Thin wrapper over existing libraries to standardize the interface for streaming uploads and downloads from Amazon S3. Supports streaming to/from variables or URLs without using the local filesystem.",
  "main": "./dist/index.js",
  "scripts": {
    "build": "babel src --presets babel-preset-es2015 --out-dir dist",
    "prepublish": "npm run build",
    "postinstall": "npm run build",
    "index": "babel-node src/index.js",
    "test": "babel-node src/test.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zwigglers/s3stream.git"
  },
  "keywords": [
    "s3",
    "stream",
    "upload",
    "download",
    "aws",
    "amazon"
  ],
  "author": "zwigglers",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zwigglers/s3stream/issues"
  },
  "homepage": "https://github.com/zwigglers/s3stream#readme",
  "dependencies": {
    "bluebird": "^3.5.0",
    "request": "^2.81.0",
    "s3-download-stream": "0.0.5",
    "s3-upload-stream": "^1.0.7"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "aws-sdk": "^2.92.0"
  }
}
